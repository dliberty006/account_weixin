.waiyuan .yusuanshengyu .shengyumoney{
  font-size: 40rpx;
}
.waiyuan .yusuan{
  position:absolute;
  top:160rpx;
  left:65rpx;
  font-size:26rpx;
  color:#fff;
  border:1px solid #fff;
  border-radius:20rpx;
  padding:7rpx;
}

.neiyuan{
	width: 600px;
	height: 50%;
	position: absolute;
	top: 50%;
	left: 0;
	
}
.neiyuan .wave{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0.2;
	animation:wave 10s linear infinite;
}
.neiyuan .wave1{
	width: 100%;
	height: 100%;
	position: absolute;
	left: -100px;
	top: 0;
	opacity: 0.2;
	animation:wave1 10s linear infinite;
}
@keyframes wave{
   0%{transform: translate3d(0px,0,0);}
   25%{transform: translate3d(-150px,0,0);}
   50%{transform: translate3d(-300px,0,0);}
   75%{transform: translate3d(-150px,0,0);}
   100%{transform: translate3d(0px,0,0);}
  
}
@keyframes wave1{
   0%{transform: translate3d(-300px,0,0);}
   25%{transform: translate3d(-150px,0,0);}
   50%{transform: translate3d(0px,0,0);}
   75%{transform: translate3d(-150px,0,0);}
   100%{transform: translate3d(-300px,0,0);}
}
