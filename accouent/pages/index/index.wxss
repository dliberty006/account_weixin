.title{
  width: 100%;
  height: 250rpx;
  background-color: #69C3AA;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}
.title .head_img{
  width: 50%;
  text-align: center;
  padding-top: 10rpx;

}
.title .head_img .ji{
  margin-top: 75rpx;
  background-color: #ff0000;
  color: #fff;
  height: 50rpx;
  line-height: 50rpx;
  width: 60%;
}
.title .head_img .image{
  width: 161rpx;
  height: 161rpx;
  border-radius: 50%;
  margin-top: 20rpx;
  display: inline-block;
  overflow: hidden;
}
.title .head_img .head_name{
  line-height: 48rpx;
  height: 48rpx;
  font-size: 30rpx;
  padding-left: 20rpx;
}
.title .head_pro{
  color: red;
  
  padding-top: 200rpx;
  float: right
}
.body_title{
  padding: 10rpx 0 10rpx 20rpx;
  width: 100%;
  height: 100rpx;
  position: fixed;
  top: 250rpx;
  left: 0;
  background-color: #fff;
  box-shadow: 0rpx 5rpx 5rpx #ddd;
   z-index: 9999;
}
.body_title .body_title_time {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}
.body_title .body_tilte_acc{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
}

.body_title text{
  color:#aaa;
  margin-right: 10rpx;
}
.body_title .fr_history{
  width: 65rpx;
  height: 70rpx;
  float: right;
  padding-right:25rpx;
  padding-top: 18rpx;
}
.body_title .fr_history .history_meng{  
  position:absolute;
  width: 65rpx;
  height: 70rpx;
  background:#FFF;
  z-index:99998;
  top:15rpx;
  right:30rpx;
  border-radius: 15rpx;
  padding: 15rpx;
}

.body {
  margin-top: 370rpx;
}
.body_desc{
  margin-top: 5rpx;
}
.body_desc .body_list .body_list_title{
  width: 100%;
  background-color: #f5f5f5;
  height: 60rpx;
}
.body_desc .body_list .body_list_title view{
  margin: 10rpx 20rpx;
}
.body_desc .body_list_desc{
  width: 95%;
  margin: 0 auto;
  overflow: hidden;
}
.body_desc .body_list_desc .body_list_desc_li {
  height: 120rpx;
  line-height: 120rpx;
}
.body_desc .body_list_desc .fl{
  width: 15%;
  text-align: center;
}
.body_desc .body_list_desc .fl .first_name{
   width: 80rpx;
   height: 80rpx;
   border-radius: 50%;
   color: #fff;
   font-size: 50rpx;
   line-height: 80rpx;
   margin-top:20rpx;
}
.body_desc .body_list_desc .fr{
  width: 85%;
  border-bottom: 1px solid #ddd;
}

.body_desc .body_list_desc .fr .left view{
  height: 50rpx;
  line-height: 50rpx;
}
.body_desc .body_list_desc .fr .right{
  font-size: 35rpx;
  font-weight: 600;
  padding-right: 20rpx;
}
.body_desc .body_list_desc .fr .remarks{
  color: #aaa;
}

.body_desc .body_list_desc .fl .red{
   background-color: #f44455;
}
.body_desc .body_list_desc .fr .red{
  color: #f44455;
}
.body_desc .body_list_desc .fl .green{
   background-color: #1aa783;
}
.meng{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 99998;
  opacity:0.5;
}

.body .time .fl_activity{
  width: 65rpx;
  height: 70rpx;
  float: right;
  padding-right: 20rpx;
  padding-top: 18rpx;
}

.delete{
  height: 150rpx;
  width: 70%;
  background: #fff;
  color: #000;
  position: fixed;
  top: 40%;
  left: 15%;
  border-radius: 10rpx;
  z-index: 99999;
  
}
.delete view{
  height: 75rpx;
  width: 100%;
  line-height: 75rpx;
  padding-left: 50rpx;
  box-sizing: border-box;
}
.delete .modify{
  border-bottom: 1px solid #ddd;
}